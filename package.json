{
  "name": "pdf-convertor-tool",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build-env": "ts-node scripts/set-env.ts",
    "build-env-prod": "NODE_ENV=production ts-node scripts/set-env.ts",
    "start": "npm run build-env && ng serve",
    "build": "npm run build-env-prod && ng build",
    "watch": "npm run build-env && ng build --watch --configuration development",
    "test": "npm run build-env && ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "file-saver": "^2.0.5",
    "ng2-pdfjs-viewer": "^17.0.3",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/cli": "^17.1.0",
    "@angular/compiler-cli": "^17.1.0",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "dotenv": "^16.4.5",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.2"
  }
